@prefix : <https://w3id.org/semanticarts/ns/ontology/operators/> .
@prefix ops: <https://w3id.org/semanticarts/ns/ontology/operators/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/semanticarts/ontology/operators/>
	a owl:Ontology ;
	skos:definition "Generic operators for specifying relationships, e.g. greater than."^^xsd:string ;
	skos:prefLabel "Operators"^^xsd:string ;
	ops:hasVersionNumber [
		a ops:SemanticVersionNumber ;
		ops:containedText "1.0.1"^^xsd:string ;
	] ;
	.

ops:equalTo
	a owl:ObjectProperty ;
	rdfs:subPropertyOf
		ops:greaterOrEqualTo ,
		ops:lessOrEqualTo
		;
	skos:definition "This is the exact numeric value."^^xsd:string ;
	skos:prefLabel "equal to"^^xsd:string ;
	.

ops:greaterOrEqualTo
	a owl:ObjectProperty ;
	rdfs:subPropertyOf ops:specifiedValue ;
	skos:definition "The value is greater than or equal to a specified value."^^xsd:string ;
	skos:prefLabel "greater or equal to"^^xsd:string ;
	.

ops:greaterThan
	a owl:ObjectProperty ;
	rdfs:subPropertyOf ops:greaterOrEqualTo ;
	skos:definition "The value is greater  than a specified value."^^xsd:string ;
	skos:prefLabel "greater than"^^xsd:string ;
	.

ops:lessOrEqualTo
	a owl:ObjectProperty ;
	rdfs:subPropertyOf ops:specifiedValue ;
	skos:definition "The value is less  than or equal to a specified value."^^xsd:string ;
	skos:prefLabel "less or equal to"^^xsd:string ;
	.

ops:lessThan
	a owl:ObjectProperty ;
	rdfs:subPropertyOf ops:lessOrEqualTo ;
	skos:definition "The value is less  than a specified value."^^xsd:string ;
	skos:prefLabel "less than"^^xsd:string ;
	.

ops:specifiedValue
	a owl:ObjectProperty ;
	skos:definition "Relates a specification (e.g. a VersionRangeSpec) to the value(s) a characteristic/parameter/variable may or must have."^^xsd:string ;
	skos:prefLabel "specified value"^^xsd:string ;
	.

ops:targetValue
	a owl:ObjectProperty ;
	rdfs:subPropertyOf ops:specifiedValue ;
	skos:definition "For when there is an ideal value in a range.  Specifeid with a variance, either a number or a percentage."^^xsd:string ;
	skos:prefLabel "target value"^^xsd:string ;
	.

ops:varianceNumber
	a owl:ObjectProperty ;
	rdfs:subPropertyOf ops:specifiedValue ;
	skos:definition "Specifies the variance around the target value as a number."^^xsd:string ;
	skos:example "Target value 100 with .5 variance implies a range of 99.5 to 100.5."^^xsd:string ;
	skos:prefLabel "variance number"^^xsd:string ;
	.

ops:variancePercentage
	a owl:ObjectProperty ;
	rdfs:subPropertyOf ops:specifiedValue ;
	skos:definition "Specifies the variance around the target value as a percentage."^^xsd:string ;
	skos:example "Target value 100 with 5% variance implies a range of 95 to 105."^^xsd:string ;
	skos:prefLabel "variance percentage"^^xsd:string ;
	.

